<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhusp.androiddemo">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.selfwidget.SelfDefindViewActivity" />
        <activity android:name=".activity.selfwidget.ViewPagerIndicatorActivity" />
        <activity android:name=".activity.selfwidget.MyWatchViewActivity" />
        <activity android:name=".activity.selfwidget.LineChartActivity" />
        <activity android:name=".activity.selfwidget.CircleScaleViewActivity" />
        <activity android:name=".activity.assistuse.AssistUseActivity" />
        <activity android:name=".activity.assistuse.FontSetActivity" />
        <activity android:name=".activity.selfwidget.DropdownViewActivity" />
        <activity android:name=".activity.extend.WidgetExtendActivity" />
        <activity android:name=".activity.extend.dragRecyclerView.RecyclerViewDragActivity" />

        <service android:name=".activity.service.CheckAppStateService" />

        <activity android:name=".activity.selfwidget.ExpandTextViewActivity" />
        <activity
            android:name=".activity.materialdesign.CollapsingToolBarActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".activity.materialdesign.MaterialDesignActivity" />
        <activity android:name=".activity.selfwidget.HorizontalVideoActivity" />
        <activity android:name=".activity.materialdesign.SimpleCoordinatorLayoutUseActivity" />

        <service
            android:name=".activity.service.ShowToastService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".activity.SocketTest.SocketUseTestActivity" />
        <activity android:name=".activity.selfwidget.DanmuTestActivity" />
        <activity android:name=".activity.extend.DiffPropertyTextActivity" />
        <activity android:name=".activity.svguse.SVGUseActivity"></activity>
    </application>

</manifest>